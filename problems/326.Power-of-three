const static auto io_sync_off = [](){
    std::ios::sync_with_stdio(false);
    std::cin.tie(nullptr);
    std::cout.tie(nullptr);
    return nullptr;
}();
class Solution {
public:
    bool isPowerOfThree(int n) {
        double a = log10(n)/log10(3);
        int b = a;
        if(b == a) return true;
        else return false;
    }
};
